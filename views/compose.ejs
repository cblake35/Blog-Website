<%- include("partials/header"); -%>

<h1> Compose </h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="titleBox" placeholder="Title" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" type="text" name="composeBox" placeholder="Write here..." rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-primary type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
